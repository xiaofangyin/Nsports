        <!--阳历的日期尺寸-->
        <attr format="integer" name="solar_size"/>
        <!--农历的日期颜色-->
        <attr format="color" name="lunar_color"/>
        <!--农历的日期尺寸-->
        <attr format="integer" name="lunar_size"/>
        <!--节日文字颜色-->
        <attr format="color" name="holiday_color"/>
        <!--选中的日期文字颜色-->
        <attr format="color" name="choose_color"/>
        <!--选中的日期背景-->
        <attr format="reference" name="day_bg"/>
        <!--单选时切换月份，是否选中上次的日期-->
        <attr format="boolean" name="switch_choose"/>
        <!--设置单选、多选-->
        <attr format="enum" name="choose_type">
            <enum name="single" value="0"/>
            <enum name="multi" value="1"/>
        </attr>
    </declare-styleable>
