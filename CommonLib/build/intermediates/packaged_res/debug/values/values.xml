    <declare-styleable name="CalendarView">
        <!--是否显示农历-->
        <attr format="boolean" name="show_lunar"/>
        <!--是否显示上月和下月-->
        <attr format="boolean" name="show_last_next"/>
        <!--是否显示节假日-->
